{
	"name": "vscode-laravel-goto",
	"displayName": "Laravel Goto",
	"description": "",
	"version": "0.0.1",
	"license": "MIT",
	"repository": "https://github.com/absszero/vscode-laravel-goto",
	"engines": {
		"vscode": "^1.37.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.vscode-laravel-goto"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.vscode-laravel-goto",
				"title": "Laravel Goto"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "extension.vscode-laravel-goto",
					"when": "editorTextFocus && editorLangId == php && resourceExtname == .php",
					"group": "navigation"
				}
			]
		},
		"keybindings": [
			{
				"command": "extension.vscode-laravel-goto",
				"key": "alt+;",
				"when": "editorTextFocus && editorLangId == php && resourceExtname == .php"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.37.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.2"
	}
}
